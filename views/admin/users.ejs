<% include header %>
    <h1 style="text-align: center;">Users</h1>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone</th>
            </tr>
        </thead>
        <tbody>
            <% for(var i=0; i<data.length; i++) {%>
                <tr>
                    <td>
                        <%= data[i].name %>
                    </td>
                    <td>
                        <%= data[i].email %>
                    </td>
                    <td>
                        <%= data[i].phone %>
                    </td>
                </tr>
                <% } %>
        </tbody>
    </table>
    </div>
    </body>

    </html>